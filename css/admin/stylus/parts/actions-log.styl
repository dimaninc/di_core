table.actions-log
	margin 15px 0
	padding 0
	border-collapse collapse
	width 100%
	thead
		th
			border 0 !important
			padding 5px 10px !important
			font-weight bold !important
	tbody
		td
			border 1px solid #000
			padding 5px 10px
			background #eee
			text-align left
	tr
		&:last-of-type
			td
				background #ffd
