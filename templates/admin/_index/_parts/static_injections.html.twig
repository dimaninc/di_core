{% if static_code_injections['js']|length %}
    <script type="text/javascript">
        {{ static_code_injections['js']|join('\n')|raw }}
    </script>
{% endif %}
{% for className in static_injections['js'] %}
    <script type="text/javascript" src="/_admin/_static/js/{{ className }}.js"></script>
    <script type="text/javascript">
        $(function() {
            new {{ className }}();
        });
    </script>
{% endfor %}
{% if static_code_injections['css']|length %}
    <style type="text/css">
        {{ static_code_injections['css']|join('\n')|raw }}
    </style>
{% endif %}
{% for cssName in static_injections['css'] %}
    <link rel="stylesheet" href="/_admin/_static/css/{{ cssName }}.css">
{% endfor %}
{% if static_code_injections['html']|length %}
    {{ static_code_injections['html']|join('\n')|raw }}
{% endif %}
