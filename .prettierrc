{
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": true,
  "quoteProps": "preserve",
  "tabWidth": 2,
  "printWidth": 85,
  "arrowParens": "always",
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.php",
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": "*.scss",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
